{
  "name": "gshark",
  "version": "0.0.1",
  "description": "Grate web interface for tshark",
  "repository": "git@github.com:m1z0r3/gshark.git",
  "author": "Koki Sato",
  "license": "MIT",
  "scripts": {
    "build": "run-p build:css build:js",
    "build:css": "run-s build:css:*",
    "build:css:node-sass": "node-sass client/styles/app.scss ./public/index.css",
    "build:css:postcss": "postcss ./public/index.css -c postcss.config.js -o ./public/index.css",
    "build:js": "webpack --display-error-details",
    "format": "run-p format:*",
    "format:css": "prettier --no-config --parser scss --single-quote --write 'client/styles/**/*.scss'",
    "format:js": "run-s format:js:*",
    "format:js:prettier": "prettier --config prettier.config.js --write 'client/javascripts/**/*.ts{,x}'",
    "format:js:tslint": "tslint --fix -p . 'client/javascripts/**/*.ts{,x}'",
    "lint": "tslint -p . 'client/javascripts/**/*.ts{,x}'",
    "watch": "run-p watch:css watch:js",
    "watch:css": "node-sass -w client/styles/app.scss ./public/index.css",
    "watch:js": "env NODE_ENV=development webpack --watch"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chart.js": "^2.7.2",
    "history": "^4.7.2",
    "moment": "^2.22.2",
    "react": "^16.5.2",
    "react-bootstrap": "^0.32.4",
    "react-chartjs-2": "^2.7.4",
    "react-datetime": "^2.15.0",
    "react-dom": "^16.5.2",
    "react-fontawesome": "^1.6.1",
    "react-redux": "^5.0.7",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-spinners": "^0.4.5",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/react": "^16.4.14",
    "@types/react-bootstrap": "^0.32.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-redux": "^6.0.9",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-router-redux": "^5.0.16",
    "autoprefixer": "^9.1.5",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "prettier": "^1.14.2",
    "ts-loader": "^5.1.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0"
  }
}
